{
  "private": true,
  "scripts": {
    "build": "yarn build:static",
    "serve": "yarn serve:static",
    "dev:env": "nuxi dev",
    "dev": "yarn dev:env --dotenv ./envs/.env.local",
    "dev:dev": "yarn dev:env --dotenv ./envs/.env.dev",
    "dev:test": "yarn dev:env --dotenv ./envs/.env.test",
    "dev:prod": "yarn dev:env --dotenv ./envs/.env.prod",
    "build:server": "nuxt build",
    "build:static": "nuxt generate && rm -rf dist && cp -r .output/public dist",
    "serve:static": "nuxt preview",
    "serve:server": "yarn prisma:apply-migration && nuxi start",
    "prepare": "prisma generate",
    "postinstall": "nuxt prepare",
    "init:env": "./scripts/copy-env.sh",
    "init:husky": "cd .. && husky install web/.husky",
    "contracts": "node ./scripts/import-contracts.js",
    "contracts:custom": "yarn run typechain --target=ethers-v5 './.contracts/custom/abis/*' --out-dir ./.contracts/custom/typechain && node ./scripts/gen-abis.js",
    "type": "nuxi typecheck",
    "lint": "prettier --check \"./**/*.{ts,tsx,vue}\"",
    "format": "prettier --write \"./**/*.{ts,tsx,vue}\"",
    "---PRISMA SCRIPTS----": "",
    "prisma:local": "env-cmd -f ./envs/.env.local prisma",
    "prisma:dev": "env-cmd -f ./envs/.env.dev prisma",
    "prisma:test": "env-cmd -f ./envs/.env.test prisma",
    "prisma:prod": "env-cmd -f ./envs/.env.prod prisma",
    "prisma:create-migration": "yarn run prisma:local migrate dev --create-only",
    "prisma:apply-migration": "yarn run prisma migrate deploy",
    "prisma:apply-migration:local": "yarn prisma:local migrate deploy",
    "prisma:apply-migration:dev": "yarn prisma:dev migrate deploy",
    "prisma:apply-migration:test": "yarn prisma:test migrate deploy",
    "prisma:apply-migration:prod": "yarn prisma:prod migrate deploy"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@mermaid-js/mermaid-cli": "^10.1.0",
    "@nuxt/devtools": "^0.6.0",
    "@pinia/nuxt": "^0.4.11",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@vueuse/core": "^10.1.2",
    "@vueuse/nuxt": "^10.1.2",
    "env-cmd": "^10.1.0",
    "husky": "^8.0.3",
    "nuxt": "^3.5.3",
    "nuxt-svgo": "^1.1.0",
    "nuxt-typed-router": "^3.1.5",
    "nuxt-windicss": "^2.6.0",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "prisma-docs-generator": "^0.7.0",
    "prisma-erd-generator": "^1.7.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sass": "^1.59.2",
    "typechain": "^8.1.1",
    "typescript": "^5.0.2",
    "vue-tsc": "^1.6.4",
    "zod-prisma": "^0.5.4"
  },
  "dependencies": {
    "@gotbit/evm": "^1.1.39",
    "@gotbit/evm-vue3": "^3.0.7",
    "@gotbit/evm-wallets": "^3.0.2",
    "@lukemorales/query-key-factory": "^1.2.0",
    "@prisma/client": "^4.11.0",
    "@tanstack/vue-query": "^4.26.1",
    "@trpc/client": "10.10.0",
    "@trpc/server": "10.10.0",
    "@walletconnect/ethereum-provider": "^2.8.6",
    "@walletconnect/modal": "^2.5.9",
    "@windicss/plugin-heropatterns": "^0.0.8",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "ethers": "5",
    "h3-zod": "^0.3.10",
    "jsonwebtoken": "^9.0.0",
    "mitt": "^3.0.0",
    "pinia": "^2.0.33",
    "prisma": "^4.13.0",
    "trpc-nuxt": "^0.6.0",
    "vee-validate": "^4.8.3",
    "vue-gtag-next": "^1.14.0",
    "vue-moment": "^4.1.0",
    "vue-toastification": "^2.0.0-rc.5",
    "zod": "^3.20.6"
  }
}
